{
  "build": {
    "template": [
      "default"
    ],
    "content": [
      {
        "files": [ "index.md", "toc.yml", "support/*.md", "portal/*.md", "inflight/**/*.md", "onboard/**/*.md", "teamsearch/*.md"]
      }
    ],
    "resource": [
      {
        "files": [ "logo.png", "favicon.ico", "**/images/**", "styles/jquery.min.map", "styles/main.*"]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "Help Center",
      "_appFooter": "<span>Copyright © 2019 TransVault<br>Generated by <strong>DocFX</strong></span>",
      "_appLogoPath": "logo.png",
      "_appFaviconPath": "favicon.ico",
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableAffix": false,
      "_disableContribution": false,
      "_enableSearch": true,
      "_enableNewTab": false
    },
    "fileMetadata": {
      "priority": {
        "**.md": 2.6,
        "portal/**.md": 1.3
      }
    },
    "dest": "wwwroot"
  }
}
